{
  "expo": {
    "name": "Money Tracking",
    "slug": "mobile-tracking",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icons/logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/icons/splash.png",
      "resizeMode": "contain"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "${PRODUCT_NAME} needs access to your Camera"
      },
      "bundleIdentifier": "com.trungnghia202.mobileapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/ic_launcher_foreground.png",
        "backgroundColor": "#ffffff"
      },
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.trungnghia202.mobileapp",
      "permissions": [
        "android.permission.CAMERA"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "${PRODUCT_NAME} needs access to your Camera"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4398ea95-cd58-4bc8-9006-5cdade34bdb0"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/4398ea95-cd58-4bc8-9006-5cdade34bdb0"
    }
  }
}
